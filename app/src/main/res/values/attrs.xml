<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingView">
        <attr name="indicator_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="AutoFitTextView">
        <attr name="text_size_min" format="dimension"/>
        <attr name="text_size_max" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="RingProgressBar">
        <attr name="ringColor" format="color" />
        <attr name="ringProgressColor" format="color" />
        <attr name="ringWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="textIsShow" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
        <attr name="type">
            <enum name="basket" value="0"/>
            <enum name="football" value="1"/>
        </attr>
    </declare-styleable>
</resources>